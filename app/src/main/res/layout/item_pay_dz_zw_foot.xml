    <com.kupo.ElephantHead.widget.EmptyLayout
        android:id="@+id/foot_empty"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone" />

    <TextView
        android:id="@+id/toDzOrZw"
